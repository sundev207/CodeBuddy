{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "jsx-a11y/anchor-is-valid": 2,
    "jsx-a11y/href-no-hash": "off",
    "allowArrowFunctions": true,
    "no-underscore-dangle": 0
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "env": {
    "browser": true
  }
}
